    "type": "git",
    "url": "git+https://github.com/blackbaud/skyux-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud/skyux-i18n/issues"
  },
  "homepage": "https://github.com/blackbaud/skyux-i18n#readme",
  "peerDependencies": {
    "@angular/core": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@skyux/assets": "^3.0.0",
    "@skyux/core": "^3.0.2",
    "rxjs": "^5.4.3"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/animations": "4.3.6",
